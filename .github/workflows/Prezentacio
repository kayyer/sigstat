name: Prezentációs példa

on:
  push:
    branches: [ develop ]
  pull_request:
    branches: [ develop ]

jobs:
  build:
    runs-on: ubuntu-latest
   
   steps:
    - uses: actions/checkout@v2
    
    - name: Ez egy lépés
      run: echo "Ez itt kiíródik"
      
    - name: Ez egy másik lépés
      uses: actions/setup-dotnet@v1
      with:
        dotnet-version: 2.2.x
    
 
  
